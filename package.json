{
  "name": "nodejs-package",
  "version": "0.3.0",
  "description": "A universal seed for nodejs package.",
  "main": "lib/index.js",
  "types": "native/index.d.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "precommit": "pretty-quick --staged",
    "prepush": "npm test",
    "prepublish": "npm-run-all -s clean compile:native compile:cjs",
    "test": "jest --coverage",
    "test:cov": "jest --watch",
    "clean": "rm -rf lib native",
    "compile:native": "tsc --declaration --outDir native",
    "compile:cjs": "babel src --source-maps --out-dir lib --extensions '.js,.ts'"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/coco-template/nodejs-package.git"
  },
  "keywords": [
    "nodejs",
    "universal"
  ],
  "author": "huang.jian <hjj491229492@hotmail.com>",
  "contributors": [
    {
      "name": "huang.jian",
      "email": "hjj491229492@hotmail.com"
    },
    {
      "name": "yang.yuncai",
      "email": "383235388@qq.com"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coco-template/nodejs-package/issues"
  },
  "homepage": "https://github.com/coco-template/nodejs-package#README",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.8",
    "@types/node": "^10.12.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "coveralls": "^3.0.2",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.8.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.4"
  },
  "dependencies": {}
}
